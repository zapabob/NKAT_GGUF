{
  "nkat_config": {
    "version": "1.0",
    "description": "NKAT理論統合設定 for GGUF",
    
    "theory_components": {
      "kolmogorov_arnold": {
        "enable": true,
        "ka_grid_size": 8,
        "activation_type": "learnable_spline",
        "quantization_bits": 8,
        "spline_order": 3,
        "boundary_conditions": "periodic"
      },
      
      "lie_algebra": {
        "enable": true,
        "dimension": 4,
        "algebra_type": "su2",
        "structure_constants_file": null,
        "auto_compute_constants": true
      },
      
      "noncommutative_geometry": {
        "enable": true,
        "strength": 0.1,
        "spectral_triple": true,
        "dixmier_trace": false
      },
      
      "differential_geometry": {
        "enable": true,
        "manifold_dimension": 2,
        "connection_type": "levi_civita",
        "curvature_regularization": 0.01
      },
      
      "spectral_theory": {
        "enable": true,
        "radius_bound": 1.0,
        "eigenvalue_regularization": 0.001,
        "functional_calculus": true
      }
    },
    
    "quantization_settings": {
      "aware_mode": true,
      "precision_preservation": true,
      "dynamic_scaling": true,
      "gradient_clipping": true,
      "numerical_stability": "high"
    },
    
    "performance_optimization": {
      "lightweight_mode": true,
      "cuda_optimization": true,
      "memory_efficiency": true,
      "inference_acceleration": true
    },
    
    "compatibility": {
      "llama_cpp_version": ">=b2008",
      "gguf_version": ">=3",
      "quantization_types": ["Q4_K_M", "Q5_K_M", "Q8_0", "F16"],
      "architectures": ["llama", "mistral", "mixtral", "qwen"]
    }
  }
} 